import java.util.Scanner;
public class StringReverse {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
		String s;
		s=sc.next();
        for(int i=s.length();i>=0;i++) {
        	System.out.println(s);
        	break;
        }
	}

}
